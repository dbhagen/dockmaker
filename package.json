{
  "name": "dockmaker",
  "description": "Dockfile generator",
  "author": {
    "name": "Daniel B. Hagen",
    "email": "daniel.b.hagen@gmail.com",
    "url": "https://danielbhagen.com"
  },
  "homepage": "https://dockmaker.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "styleguide": "vue-cli-service styleguidist",
    "styleguide:build": "vue-cli-service styleguidist:build"
  },
  "dependencies": {
    "aws-amplify": "^2.2.6",
    "aws-amplify-vue": "^1.1.4",
    "aws-appsync": "^3.0.2",
    "core-js": "^3.6.4",
    "graphql": "^14.6.0",
    "octonode": "^0.9.5",
    "portal-vue": "^2.1.7",
    "vue": "^2.6.11",
    "vue-apollo": "^3.0.3",
    "vue-cli-plugin-amplify": "^0.1.0",
    "vue-i18n": "^8.0.0",
    "vue-router": "^3.1.6",
    "vuedraggable": "^2.23.2",
    "vuetify": "^2.2.11",
    "vuex": "^3.1.2",
    "vuex-persistedstate": "^2.7.1"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@storybook/addon-actions": ">=5.3.0",
    "@storybook/addon-docs": ">=5.3.0",
    "@storybook/addon-knobs": ">=5.3.0",
    "@storybook/addon-links": ">=5.3.0",
    "@storybook/addon-notes": ">=5.3.0",
    "@storybook/vue": ">=5.3.0",
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-router": "~4.2.0",
    "@vue/cli-plugin-vuex": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "babel-eslint": "^10.0.3",
    "del": "^5.1.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-graphql": "^2.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "eslint-plugin-vue": "^6.2.2",
    "graphql-tag": "^2.9.0",
    "gulp": "^4.0.2",
    "gulp-favicons": "^2.4.0",
    "gulp-inject": "^5.0.5",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-i18n": "^0.6.1",
    "vue-cli-plugin-storybook": "~1.2.1",
    "vue-cli-plugin-styleguidist": "~4.13.1",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.4.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "airbnb-base",
      "eslint:recommended",
      "plugin:vue/recommended"
    ],
    "plugins": [
      "simple-import-sort"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "simple-import-sort/sort": "error",
      "sort-imports": "off",
      "import/order": "off"
    }
  },
  "eslintIgnore": [
    "node_modules/*",
    "src/aws-exports.js"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
